<html>
<!--HEAD-->
<%- include(".././partials/headpeliculas2023") %>

  <body class="bg1">
    <!--HEADER-->
    <%- include(".././partials/header") %>
<main>
      <!--MAIN-->
      <h1 class="tituloDetalle">PELICULAS 2023</h1>
      <div class="row row-cols-1 row-cols-md-2 ">

          <div class="col mb-4">
            <!-- Contenido -->
            <div class="card">
              <% for(let i=0; i<datos.length ; i++) {%>
                <!--Titulo-->
                <h4 class="card-title">
                  <%= datos[i].nombre %>
                </h4>
                <!--Contenido imagen-->
                <div class="view overlay">
                  <div><iframe width="855" height="550" src="<%= datos[i].video %>" title="YouTube video player"
                      frameborder="0"
                      allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                      allowfullscreen></div> </iframe>
                  <div class="mask rgba-white-slight"></div>

                </div>
                <h6 class="card-text"><%=datos[i].fichatecnica%></h6>
                <h6 class="card-text"><%=datos[i].fichatecnica2%></h6>
                <h6 class="card-text"><%=datos[i].fichatecnica7%></h6>
                <h6 class="card-text"><%=datos[i].fichatecnica9%></h6>
                <!--Texto-->
                <p class="card-text">
                  <%=datos[i].descripcion%>
                </p>
                <br>
            </div>
          <% } %>

            </main>
            
            <!-- FOOTER -->
            <%- include(".././partials/footer.ejs")%>
  </body>

</html>